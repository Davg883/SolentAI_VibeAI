{
  "version": 2,
  "routes": [
    {
      "src": "/Images/(.*)",
      "dest": "/Images/$1"
    },
    {
      "src": "/style.css",
      "dest": "/vibe-pitch-app/style.css"
    },
    {
      "src": "/script.js",
      "dest": "/vibe-pitch-app/script.js"
    },
    {
      "src": "/(.*)",
      "dest": "/vibe-pitch-app/index.html"
    }
  ]
}
